  "components": {
    "schemas": {
      "AppendFileRequest": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path",
            "description": "Path to the file to append to"
          },
          "content": {
            "type": "string",
            "title": "Content",
            "description": "Content to append"
          }
        },
        "type": "object",
        "required": [
          "path",
          "content"
        ],
        "title": "AppendFileRequest"
      },
      "CalculateRequest": {
        "properties": {
          "expression": {
            "type": "string",
            "title": "Expression",
            "description": "Math expression to evaluate"
          }
        },
        "type": "object",
        "required": [
          "expression"
        ],
        "title": "CalculateRequest"
      },
      "CountWordsRequest": {
        "properties": {
          "text": {
            "type": "string",
            "title": "Text",
            "description": "Text to analyze"
          }
        },
        "type": "object",
        "required": [
          "text"
        ],
        "title": "CountWordsRequest"
      },
      "CreateDirectoryRequest": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path",
            "description": "Directory path to create"
          },
          "parents": {
            "type": "boolean",
            "title": "Parents",
            "description": "Create parent directories",
            "default": true
          }
        },
        "type": "object",
        "required": [
          "path"
        ],
        "title": "CreateDirectoryRequest"
      },
      "DeleteFileRequest": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path",
            "description": "Path to the file to delete"
          }
        },
        "type": "object",
        "required": [
          "path"
        ],
        "title": "DeleteFileRequest"
      },
      "FindFilesRequest": {
        "properties": {
          "directory": {
            "type": "string",
            "title": "Directory",
            "description": "Directory to search",
            "default": "."
          },
          "pattern": {
            "type": "string",
            "title": "Pattern",
            "description": "Glob pattern",
            "default": "*"
          },
          "recursive": {
            "type": "boolean",
            "title": "Recursive",
            "description": "Recursive search",
            "default": true
          },
          "max_results": {
            "type": "integer",
            "title": "Max Results",
            "description": "Max results",
            "default": 100
          }
        },
        "type": "object",
        "title": "FindFilesRequest"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ListDirectoryRequest": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path",
            "description": "Directory path",
            "default": "."
          },
          "show_hidden": {
            "type": "boolean",
            "title": "Show Hidden",
            "description": "Show hidden files",
            "default": false
          },
          "detailed": {
            "type": "boolean",
            "title": "Detailed",
            "description": "Show detailed info",
            "default": false
          }
        },
        "type": "object",
        "title": "ListDirectoryRequest"
      },
      "OrganizeDirectoryRequest": {
        "properties": {
          "directory": {
            "type": "string",
            "title": "Directory",
            "description": "Directory to organize",
            "default": "."
          },
          "by": {
            "type": "string",
            "title": "By",
            "description": "Method: type, date, size",
            "default": "type"
          },
          "dry_run": {
            "type": "boolean",
            "title": "Dry Run",
            "description": "Simulate only",
            "default": false
          }
        },
        "type": "object",
        "title": "OrganizeDirectoryRequest"
      },
      "ReadFileRequest": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path",
            "description": "Path to the file to read"
          }
        },
        "type": "object",
        "required": [
          "path"
        ],
        "title": "ReadFileRequest"
      },
      "SearchTextRequest": {
        "properties": {
          "text": {
            "type": "string",
            "title": "Text",
            "description": "Text to search in"
          },
          "query": {
            "type": "string",
            "title": "Query",
            "description": "Query string"
          },
          "case_sensitive": {
            "type": "boolean",
            "title": "Case Sensitive",
            "description": "Case sensitive search",
            "default": false
          }
        },
        "type": "object",
        "required": [
          "text",
          "query"
        ],
        "title": "SearchTextRequest"
      },
      "UpdateFileRequest": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path",
            "description": "Path to the file to update"
          },
          "old_text": {
            "type": "string",
            "title": "Old Text",
            "description": "Text to find"
          },
          "new_text": {
            "type": "string",
            "title": "New Text",
            "description": "Replacement text"
          },
          "count": {
            "type": "integer",
            "title": "Count",
            "description": "Max replacements (-1 for all)",
            "default": -1
          }
        },
        "type": "object",
        "required": [
          "path",
          "old_text",
          "new_text"
        ],
        "title": "UpdateFileRequest"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "WriteFileRequest": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path",
            "description": "Path to the file to write"
          },
          "content": {
            "type": "string",
            "title": "Content",
            "description": "Content to write"
          },
          "overwrite": {
            "type": "boolean",
            "title": "Overwrite",
            "description": "Overwrite existing file",
            "default": true
          }
        },
        "type": "object",
        "required": [
          "path",
          "content"
        ],
        "title": "WriteFileRequest"
      }
    }
  }
